{
    "name": "MultiObjectiveDiagram_options",
    "required": ["diagram","scalar_name", "weights"],
    "properties": {
        "diagram": {
            "type": "string"
        },
        "scalar_name": {
            "type": "string"
        },
        "weights": {
            "oneOf": [
                {"$ref": "#/$defs/array_of_numbers"},
                {
                    "type": "array",
                    "items": {"$ref": "#/$defs/array_of_numbers"}
                }
            ]
        },
        "normalize_each_objective": {
            "type": "boolean"
        }
    },
    "$defs": {
        "array_of_numbers": {
            "type": "array",
            "items": {
                "type": "number",
                "minItems": 2
            }
        }
    },
    "additionalProperties": false,
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object"
}
